<?xml version="1.0"?>
<robot name="bottle">

  <link name="bottle_link">
    <visual>
      <geometry>
        <cylinder radius="0.03" length="0.13"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <material name="blue">
        <color rgba="0.1 0.4 0.9 1.0"/>
      </material>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.03" length="0.13"/>
      </geometry>
      <surface>
        <friction>
          <ode mu="2.0" mu2="2.0"/>
        </friction>
        <bounce restitution_coefficient="0.0"/>
      </surface>
    </collision>

    <inertial>
      <mass value="0.3"/>
      <origin xyz="0 0 -0.04" rpy="0 0 0"/>
      <inertia ixx="1.5e-04" iyy="1.5e-04" izz="3.0e-05"
               ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>
</robot>

